{
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "json",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "yaml"
  ],
  "languageserver": {
    "dls": {
      "args": [
        "--stdio"
      ],
      "command": "diagnostic-languageserver",
      "filetypes": [
        "json",
        "sh"
      ],
      "initializationOptions": {
        "filetypes": {},
        "formatFiletypes": {
          "json": [
            "json-fixjson",
            "json-jq"
          ]
        },
        "formatters": {
          "json-fixjson": {
            "command": "fixjson",
            "isStderr": false,
            "isStdout": true
          },
          "json-jq": {
            "args": [
              ".",
              "--sort-keys"
            ],
            "command": "jq",
            "isStderr": false,
            "isStdout": true
          }
        },
        "linters": {}
      }
    },
    "golang": {
      "command": "gopls",
      "filetypes": [
        "go"
      ],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "settings": {
        "completeUnimported": true,
        "usePlaceholders": true
      }
    }
  },
  "signature.maxWindowHeight": 32,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ]
}
