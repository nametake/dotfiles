{
  "prettier.disableLanguages": ["json", "markdown", "graphql"],
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "go",
    "json",
    "yaml"
  ],
  "languageserver": {
    "dls": {
      "command": "diagnostic-languageserver",
      "args": ["--stdio"],
      "filetypes": [ "sh", "email", "json" ],
      "initializationOptions": {
        "linters": {
        },
        "filetypes": {
        },
        "formatters": {
          "json-fixjson": {
            "command": "fixjson",
            "isStdout": true,
            "isStderr": false
          },
          "json-jq": {
            "command": "jq",
            "args": [".", "--sort-keys"],
            "isStdout": true,
            "isStderr": false
          }
        },
        "formatFiletypes": {
          "json": ["json-fixjson", "json-jq"]
        }
      }
    },
    "golang": {
      "command": "gopls",
      "filetypes": [
        "go"
      ],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "settings": {
        "completeUnimported": true,
        "usePlaceholders": true
      }
    }
  },
  "signature.maxWindowHeight": 32,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ]
}
