{
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "json",
    "markdown",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "yaml"
  ],
  "languageserver": {
    "dls": {
      "args": [
        "--stdio"
      ],
      "command": "diagnostic-languageserver",
      "filetypes": [
        "json",
        "markdown",
        "sh",
        "vim"
      ],
      "initializationOptions": {
        "filetypes": {
          "make": [
            "make-checkmake"
          ],
          "markdown": [
            "markdown-remark-lint"
          ],
          "vim": [
            "vim-vint"
          ]
        },
        "formatFiletypes": {
          "json": [
            "json-fixjson",
            "json-jq"
          ],
          "markdown": [
            "markdown-remark"
          ]
        },
        "formatters": {
          "json-fixjson": {
            "command": "fixjson",
            "isStderr": false,
            "isStdout": true
          },
          "json-jq": {
            "args": [
              ".",
              "--sort-keys"
            ],
            "command": "jq",
            "isStderr": false,
            "isStdout": true
          },
          "markdown-remark": {
            "args": [
              "--no-color",
              "--silent"
            ],
            "command": "remark"
          }
        },
        "linters": {
          "markdown-remark-lint": {
            "args": [
              "--quiet",
              "--no-stdout",
              "--no-color"
            ],
            "command": "remark",
            "formatPattern": [
              "^ +(\\d+):(\\d+)-(\\d+):(\\d+)  (warning|error)  (.+?(?=  +)) +(.+?(?=  +)) +(.+)$",
              {
                "column": 2,
                "endColumn": 4,
                "endLine": 3,
                "line": 1,
                "message": [
                  6,
                  " ",
                  7,
                  " ",
                  8
                ],
                "security": 5
              }
            ],
            "isStderr": true,
            "securities": {
              "error": "error",
              "note": "info",
              "warning": "warning"
            },
            "sourceName": "remark"
          },
          "vim-vint": {
            "args": [
              "--enable-neovim",
              "-"
            ],
            "command": "vint",
            "debounce": 100,
            "formatLines": 1,
            "formatPattern": [
              "[^:]+:(\\d+):(\\d+):\\s*(.*)(\\r|\\n)*$",
              {
                "column": 2,
                "line": 1,
                "message": 3
              }
            ],
            "offsetColumn": 0,
            "offsetLine": 0,
            "sourceName": "vint"
          }
        }
      }
    },
    "golang": {
      "command": "gopls",
      "filetypes": [
        "go"
      ],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "settings": {
        "completeUnimported": true,
        "usePlaceholders": true
      }
    }
  },
  "signature.maxWindowHeight": 32,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ]
}
