{
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "json",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "yaml"
  ],
  "languageserver": {
    "dls": {
      "args": [
        "--stdio"
      ],
      "command": "diagnostic-languageserver",
      "filetypes": [
        "json",
        "sh",
        "vim"
      ],
      "initializationOptions": {
        "filetypes": {
          "make": [
            "make-checkmake"
          ],
          "vim": [
            "vim-vint"
          ]
        },
        "formatFiletypes": {
          "json": [
            "json-fixjson",
            "json-jq"
          ]
        },
        "formatters": {
          "json-fixjson": {
            "command": "fixjson",
            "isStderr": false,
            "isStdout": true
          },
          "json-jq": {
            "args": [
              ".",
              "--sort-keys"
            ],
            "command": "jq",
            "isStderr": false,
            "isStdout": true
          }
        },
        "linters": {
          "vim-vint": {
            "args": [
              "--enable-neovim",
              "-"
            ],
            "command": "vint",
            "debounce": 100,
            "formatLines": 1,
            "formatPattern": [
              "[^:]+:(\\d+):(\\d+):\\s*(.*)(\\r|\\n)*$",
              {
                "column": 2,
                "line": 1,
                "message": 3
              }
            ],
            "offsetColumn": 0,
            "offsetLine": 0,
            "sourceName": "vint"
          }
        }
      }
    },
    "golang": {
      "command": "gopls",
      "filetypes": [
        "go"
      ],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "settings": {
        "completeUnimported": true,
        "usePlaceholders": true
      }
    }
  },
  "signature.maxWindowHeight": 32,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ]
}
