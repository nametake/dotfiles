{
  "diagnostic.displayByAle": true,
  "signature.maxWindowHeight": 32,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ],
  "languageserver": {
    "golang": {
      "command": "go-langserver",
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    }
  }
}
