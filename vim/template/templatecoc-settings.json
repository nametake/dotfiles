{
  "languageserver": {
    "dls": {
      "command": "diagnostic-languageserver",
      "filetypes": [
        "dockerfile",
        "go",
        "json",
        "markdown",
        "sh",
        "yaml",
        "vim"
      ],
      "initializationOptions": {
        "formatFiletypes": {
          "json": [
            "json-fixjson"
          ]
        }
      }
    }
  }
}
