[
  {
    "command": "editor.action.quickFix",
    "key": ";",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly && vim.mode == 'Normal'"
  },
  {
    "command": "-workbench.action.toggleEditorGroupLayout",
    "key": "alt+cmd+0"
  },
  {
    "command": "-workbench.action.nextEditor",
    "key": "alt+cmd+right"
  },
  {
    "command": "-editor.action.commentLine",
    "key": "cmd+/",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-workbench.action.focusBelowGroup",
    "key": "cmd+k cmd+down"
  },
  {
    "command": "-workbench.action.focusLeftGroup",
    "key": "cmd+k cmd+left"
  },
  {
    "command": "-workbench.action.focusRightGroup",
    "key": "cmd+k cmd+right"
  },
  {
    "command": "-workbench.action.focusAboveGroup",
    "key": "cmd+k cmd+up"
  },
  {
    "command": "-workbench.action.quickOpen",
    "key": "cmd+p"
  },
  {
    "command": "editor.action.commentLine",
    "key": "ctrl+- ctrl+-",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "-explorer.openToSide",
    "key": "ctrl+enter",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  {
    "command": "workbench.action.focusLeftGroup",
    "key": "ctrl+h"
  },
  {
    "command": "workbench.action.focusBelowGroup",
    "key": "ctrl+j"
  },
  {
    "command": "acceptSelectedSuggestion",
    "key": "ctrl+k",
    "when": "suggestWidgetVisible && textInputFocus"
  },
  {
    "command": "insertSnippet",
    "key": "ctrl+k",
    "when": "editorTextFocus && hasSnippetCompletions && !editorTabMovesFocus && !inSnippetMode"
  },
  {
    "command": "jumpToNextSnippetPlaceholder",
    "key": "ctrl+k",
    "when": "editorTextFocus && hasNextTabstop && inSnippetMode"
  },
  {
    "command": "workbench.action.focusAboveGroup",
    "key": "ctrl+k"
  },
  {
    "command": "workbench.action.focusRightGroup",
    "key": "ctrl+l"
  },
  {
    "command": "workbench.action.quickOpen",
    "key": "ctrl+p",
    "when": "!suggestWidgetVisible && editorTextFocus"
  },
  {
    "command": "workbench.action.quickOpen",
    "key": "ctrl+p",
    "when": "!suggestWidgetVisible && filesExplorerFocus"
  },
  {
    "command": "workbench.action.findInFiles",
    "key": "ctrl+s"
  },
  {
    "command": "workbench.action.nextEditor",
    "key": "ctrl+t n"
  },
  {
    "command": "workbench.action.previousEditor",
    "key": "ctrl+t p"
  },
  {
    "command": "workbench.action.toggleEditorGroupLayout",
    "key": "ctrl+w space"
  },
  {
    "command": "-renameFile",
    "key": "enter",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "list.select",
    "key": "enter",
    "when": "explorerViewletFocus && !inputFocus && filesExplorerFocus"
  },
  {
    "command": "list.toggleExpand",
    "key": "enter",
    "when": "explorerViewletFocus && !inputFocus && explorerResourceIsFolder"
  },
  {
    "command": "-editor.action.rename",
    "key": "f2",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "command": "explorer.newFile",
    "key": "m a",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "command": "deleteFile",
    "key": "m d",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "command": "renameFile",
    "key": "m m",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "explorer.openToSide",
    "key": "s",
    "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
  },
  {
    "command": "-workbench.action.previousEditor",
    "key": "shift+cmd+["
  },
  {
    "command": "-workbench.view.explorer",
    "key": "shift+cmd+e"
  },
  {
    "command": "-workbench.action.findInFiles",
    "key": "shift+cmd+f"
  },
  {
    "command": "extension.changeCase.commands",
    "key": "space c",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "command": "workbench.view.explorer",
    "key": "space f",
    "when": "editorFocus && vim.mode == 'Normal'"
  },
  {
    "command": "editor.action.marker.next",
    "key": "space j",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly && vim.mode == 'Normal'"
  },
  {
    "command": "editor.action.marker.prev",
    "key": "space k",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly && vim.mode == 'Normal'"
  },
  {
    "command": "editor.action.rename",
    "key": "space r",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly && vim.mode == 'Normal'"
  },
  {
    "command": "list.collapse",
    "key": "x",
    "when": "explorerViewletFocus && !inputFocus"
  }
]
